   "source": [
    "t = int(input())\n",
    "for i in range(t):\n",
    "    case = list(input().split())\n",
    "    a = float(case[0])\n",
    "    for j in range(1, len(case)):\n",
    "        if case[j] == \"@\":\n",
    "            a *= 3\n",
    "        elif case[j] == \"%\":\n",
    "            a += 5\n",
    "        else:\n",
    "            a -= 7\n",
    "    print(\"%.2f\" % a)"
   ]
